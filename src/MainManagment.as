package{	import flash.display.MovieClip;	import flash.utils.Timer;	import flash.events.TimerEvent;	import flash.geom.Rectangle;	import LinkEngine;	import characterStatus;		public class MainManagment extends MovieClip {		var wizardObject:MovieClip;		var playerObject:MovieClip;		var wizardFollowTimer:Timer;		var camObject:MovieClip;				public function MainManagment() {			// constructor code			playerObject = (root as MovieClip).player;			var monsterObject:MovieClip  = (root as MovieClip).monster;			var monsterObject2:MovieClip = (root as MovieClip).monster3;						wizardObject = (root as MovieClip).wizard;			camObject	 = (root as MovieClip).cam;						var linkEngin:LinkEngine;						var player:characterStatus	 = new characterStatus(playerObject);			player.powerPoint = 150;//30;						var monster1:characterStatus = new characterStatus(monsterObject);			monster1.healthPoint = 1500;			monster1.health = 1500;			monster1.characterName = "TestMonster1";			monster1.backPoint = 900;						wizardFollowTimer = new Timer(0);			wizardFollowTimer.addEventListener(TimerEvent.TIMER, wizardFollow);			wizardFollowTimer.start();						/*			monster2 = new characterStatus(monsterObject2);			monster2.healthPoint = 1800;			monster2.health = 1800;			monster2.characterName = "TestMonster2";			monster2.backPoint = 600;			*/						var blockArray:Array = objectGrouping("block");						var linkEngin:LinkEngine = new LinkEngine(root, player, monster1, blockArray);		}				public function wizardFollow(e:TimerEvent):void		{			//var temp = barTarget.health/barTarget.healthPoint;			//var bossMonsterBar:MovieClip = (mainTimeLine as MovieClip).bossMonsterBar;			//var barMask:MovieClip = bossMonsterBar.barMask;			wizardObject.x = wizardObject.x + 0.05*(playerObject.x-(wizardObject.x+200));			wizardObject.y = wizardObject.y + 0.05*(playerObject.y-(wizardObject.y+300));			//camObject.x = playerObject.x; //camObject.x + 0.1*(playerObject.x-camObject.x);			//camObject.y = playerObject.y; //camObject.y + 0.1*(playerObject.y-camObject.y);		}//function wizardFollow - 마법사가 따라다님				public function objectGrouping(objectName:String):Array		{			var objectGroup:Array = new Array();			var i=1;			do{				if(root.hasOwnProperty(objectName+i) == false) break;				else										   objectGroup.push((root as MovieClip)[objectName+i]);				i++;			}while(1);						return objectGroup;		}				public function objectCount(objectName:String):Number		{			var i=1;			var objectCount=0;			do{				if(root.hasOwnProperty(objectName+i) == false) break;				else										   objectCount++;				i++;			}while(1);						return objectCount;		}//function objectCount- 무비클립의 오브젝트 수를 카운트 해주는 함수(오브젝트의 이름은 mon1 식으로 뒤에 숫자가 붙어야 한다, 1부터시작)	}	}